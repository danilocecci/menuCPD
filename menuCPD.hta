         function executaListarImpressoras() {
            var marcaSelecionada = document.getElementById('printersName').value;
            var modelosImpressoras = document.getElementById('printersModel');

            function getModels(){
                  modelosImpressoras.innerHTML = ""

                  for(i = 0; i < modelos.length; i++) {
                     var option = document.createElement("option")
                     option.value = modelos[i].name;
                     option.text = modelos[i].name;
                     modelosImpressoras.add(option);
                  }
            }

            switch (marcaSelecionada) {
               case 'brother':
                  var modelos = printers[0].model;
                  getModels();
               break;

               case 'canon':
                  var modelos = printers[1].model;
