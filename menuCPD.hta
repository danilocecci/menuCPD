               return
            }

            var indexImpressora;
            var indexModelo;

            for(i = 0; i < printers.length; i++){
                  if(printers[i].name.toLocaleLowerCase() == marcaImpressoraSelecionada){
                     indexImpressora = i
                  }
               }
             
            for(i = 0; i < printers[indexImpressora].model.length; i++){
               if(printers[indexImpressora].model[i].name == impressoraSelecionada){
                  indexModelo = i
               }
            }

            var result = printers[indexImpressora].model[indexModelo].url
            
            terminal.Run(result)
         }

      </SCRIPT>


